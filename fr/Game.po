#. Key:	Biger01
#. SourceLocation:	/Game/LocalizationText/Alienation.Alienation
#: /Game/LocalizationText/Alienation.Alienation
msgctxt "Alienation,Biger01"
msgid "<Green18>+100%</>经验获取\r\n<Red18>+60%怪物伤害</>"
msgstr "<Green18>+100%</> gain d'expérience\r\n<Red18>+60% de dégâts des monstres</>"

#. Key:	Efficientlearning
#. SourceLocation:	/Game/LocalizationText/Alienation.Alienation
#: /Game/LocalizationText/Alienation.Alienation
msgctxt "Alienation,Efficientlearning"
msgid "高效学习"
msgstr "Apprentissage efficace"

#. Key:	Efficientlearning01
#. SourceLocation:	/Game/LocalizationText/Alienation.Alienation
#: /Game/LocalizationText/Alienation.Alienation
msgctxt "Alienation,Efficientlearning01"
msgid "<Green18>+2500</>经验（不受经验获取影响）\r\n<Red18>-1000%</>经验获取"
msgstr "<Green18>+2500</> expérience (non affecté par le gain d'expérience)\r\n<Red18>-1000%</> gain d'expérience"

#. Key:	Elitekillers
