#: /Game/LocalizationText/Alienation.Alienation
msgctxt "Alienation,Lurethesnakeoutofthehole"
msgid "引蛇出洞"
msgstr "Attirer le serpent hors de son trou"

#. Key:	Lurethesnakeoutofthehole01
#. SourceLocation:	/Game/LocalizationText/Alienation.Alienation
#: /Game/LocalizationText/Alienation.Alienation
msgctxt "Alienation,Lurethesnakeoutofthehole01"
msgid "下1波将会出现1只<Red18>Boss</>（Boss生命值<Green18>-30%</>）"
msgstr "La prochaine vague aura un <Red18>Boss</> (PV du Boss <Green18>-30%</>)"

#. Key:	MarvelJackpot
#. SourceLocation:	/Game/LocalizationText/Alienation.Alienation
#: /Game/LocalizationText/Alienation.Alienation
msgctxt "Alienation,MarvelJackpot"
